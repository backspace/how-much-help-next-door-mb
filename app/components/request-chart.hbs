<svg
  width="100%"
  height="100%"
  {{did-insert this.storeSvg}}
>
  {{#each this.requestRects as |requestRect|}}
    <Request
      @x={{requestRect.x}}
      @y={{requestRect.y}}
      @width={{requestRect.width}}
      @height={{requestRect.height}}
      @request={{requestRect.request}}
      {{on "mouseover" (fn @setActiveRequest requestRect.request)}}
      {{on "mouseout" (fn @setActiveRequest 0)}}
    />
  {{/each}}
</svg>